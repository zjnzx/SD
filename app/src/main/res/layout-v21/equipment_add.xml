<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MyActivity"
    android:id="@+id/drawer"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:background="@color/theme_default_primary"
            android:layout_width="match_parent"
            android:layout_height="80dp">
            <LinearLayout
                android:id="@+id/menu_back"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:background="@drawable/button_rect_list_normal"
                android:layout_alignParentLeft="true"
                android:layout_alignParentBottom="true"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/menu_back_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_arrow_back_white_24dp"/>
            </LinearLayout>

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#fff"
                android:textSize="20sp"
                android:layout_marginTop="41dp"
                android:text="新增变压器"
                android:layout_toRightOf="@id/menu_back"/>
            <LinearLayout
                android:id="@+id/menu_check"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:background="@drawable/button_rect_list_normal"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/menu_check_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_check_white_24dp"/>
            </LinearLayout>

        </RelativeLayout>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/relativeLayout"
            android:background="@drawable/capture_card_bg"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/equipmentnametext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="变压器名称"
                    android:textSize="18sp"
                    android:textColor="#919191"/>
                <com.energyfuture.symphony.m3.ui.MyEditText
                    android:id="@+id/equipmentname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="5dp"
                    android:background="@null"
                    android:textSize="18sp"
                    android:hint="必填项"
                    android:textColorHint="#DDDDDC"
                    android:textColor="#000000"
                    android:tag="变压器名称"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="3dp">
                        <TextView
                            android:id="@+id/ccbhtext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="出厂编号"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/ccbh"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:textSize="18sp"
                            android:hint="必填项"
                            android:textColorHint="#DDDDDC"
                            android:layout_below="@id/ccbhtext"
                            android:text="200310070"
                            android:textColor="#000000"
                            android:tag="出厂编号"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp">
                        <TextView
                            android:id="@+id/connectnumbertext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="连接组标号"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/connectnumber"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:textSize="18sp"
                            android:hint="必填项"
                            android:textColorHint="#DDDDDC"
                            android:layout_below="@id/connectnumbertext"
                            android:text="YNyn0d11"
                            android:textColor="#000000"
                            android:tag="连接组标号"/>
                        </RelativeLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/modeltext"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="型号"
                    android:textSize="18sp"
                    android:textColor="#919191"/>
                <com.energyfuture.symphony.m3.ui.MyEditText
                    android:id="@+id/model"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="5dp"
                    android:background="@null"
                    android:textSize="18sp"
                    android:hint="必填项"
                    android:digits="1234567890abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                    android:textColorHint="#DDDDDC"
                    android:text="SFSZ10-180000/220"
                    android:textColor="#000000"
                    android:tag="型号"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="3dp">
                        <TextView
                            android:id="@+id/phasecounttext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="相数"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/phasecount"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:textSize="18sp"
                            android:hint="必填项"
                            android:textColorHint="#DDDDDC"
                            android:layout_below="@id/phasecounttext"
                            android:text="3相"
                            android:textColor="#000000"
                            android:tag="相数"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp">
                        <TextView
                            android:id="@+id/frequencytext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="频率"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/frequency"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_marginRight="25dp"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:inputType="numberDecimal"
                            android:textSize="18sp"
                            android:hint="必填项"
                            android:textColorHint="#DDDDDC"
                            android:layout_below="@id/frequencytext"
                            android:text="50"
                            android:textColor="#000000"
                            android:tag="频率"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_below="@id/frequencytext"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="5dp"
                            android:text="HZ"
                            android:textSize="18dp"
                            android:textColor="#919191"/>
                    </RelativeLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/voltagetext"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="额定电压"
                    android:textSize="18sp"
                    android:textColor="#919191"/>
                <RelativeLayout
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.energyfuture.symphony.m3.ui.MyEditText
                        android:id="@+id/voltage"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="5dp"
                        android:layout_marginRight="25dp"
                        android:background="@null"
                        android:digits="1234567890.+-*/%_()"
                        android:inputType="numberSigned"
                        android:textSize="18sp"
                        android:hint="必填项"
                        android:textColorHint="#DDDDDC"
                        android:text="(220±8×1.25%)/121/10.5"
                        android:textColor="#000000"
                        android:tag="额定电压"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:paddingBottom="5dp"
                        android:text="kV"
                        android:textSize="18dp"
                        android:textColor="#919191"/>
                    </RelativeLayout>
                <TextView
                    android:id="@+id/ratedcapacitytext"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="额定容量"
                    android:textSize="18sp"
                    android:textColor="#919191"/>
                <RelativeLayout
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.energyfuture.symphony.m3.ui.MyEditText
                        android:id="@+id/ratedcapacity"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingBottom="5dp"
                        android:layout_marginRight="40dp"
                        android:background="@null"
                        android:inputType="numberSigned"
                        android:digits="1234567890.+-*/%_()"
                        android:textSize="18sp"
                        android:hint="必填项"
                        android:textColorHint="#DDDDDC"
                        android:text="180000/180000/54000"
                        android:textColor="#000000"
                        android:tag="额定容量"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:paddingBottom="5dp"
                        android:text="MVA"
                        android:textSize="18dp"
                        android:textColor="#919191"/>
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="3dp">
                        <TextView
                            android:id="@+id/useconditiontext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="使用条件"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/usecondition"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:textSize="18sp"
                            android:hint="必填项"
                            android:textColorHint="#DDDDDC"
                            android:layout_below="@id/useconditiontext"
                            android:text="户外"
                            android:textColor="#000000"
                            android:tag="使用条件"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp">
                        <TextView
                            android:id="@+id/oiltext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="变压器油"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/oil"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:inputType="numberSigned"
                            android:digits="#"
                            android:textSize="18sp"
                            android:hint="必填项"
                            android:textColorHint="#DDDDDC"
                            android:layout_below="@id/oiltext"
                            android:text="25#"
                            android:textColor="#000000"
                            android:tag="变压器油"/>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="3dp">
                        <TextView
                            android:id="@+id/coolmethodtext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="冷却条件"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/coolmethod"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:textSize="18sp"
                            android:hint="必填项"
                            android:textColorHint="#DDDDDC"
                            android:layout_below="@id/coolmethodtext"
                            android:text="ONAN/ONAF"
                            android:textColor="#000000"
                            android:tag="冷却条件"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp">
                        <TextView
                            android:id="@+id/voltagesetmodeltext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="调压方式"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/voltagesetmodel"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:textSize="18sp"
                            android:hint="必填项"
                            android:textColorHint="#DDDDDC"
                            android:layout_below="@id/voltagesetmodeltext"
                            android:text="有载调压"
                            android:textColor="#000000"
                            android:tag="调压方式"/>
                    </RelativeLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/companytext"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="制造厂"
                    android:textSize="18sp"
                    android:textColor="#919191"/>
                <com.energyfuture.symphony.m3.ui.MyEditText
                    android:id="@+id/company"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="5dp"
                    android:background="@null"
                    android:textSize="18sp"
                    android:hint="必填项"
                    android:textColorHint="#DDDDDC"
                    android:text="山东电力设备厂"
                    android:textColor="#000000"
                    android:tag="制造厂"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="3dp">
                        <TextView
                            android:id="@+id/productiondatetext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="制造日期"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/productiondate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:singleLine="true"
                            android:textColorHint="#DDDDDC"
                            android:textColor="#000000"
                            android:layout_below="@id/productiondatetext"
                            android:focusableInTouchMode="false"
                            android:editable="false"
                            android:layout_marginTop="10dp"
                            android:tag="制造日期"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/date_layout"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp">
                        <TextView
                            android:id="@+id/usedatetext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="投运日期"
                            android:textSize="18sp"
                            android:textColor="#919191"/>
                        <com.energyfuture.symphony.m3.ui.MyEditText
                            android:id="@+id/usedate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingBottom="5dp"
                            android:background="@null"
                            android:singleLine="true"
                            android:textColorHint="#DDDDDC"
                            android:textColor="#000000"
                            android:layout_below="@id/usedatetext"
                            android:focusableInTouchMode="false"
                            android:editable="false"
                            android:layout_marginTop="10dp"
                            android:tag="投运日期"/>
                    </RelativeLayout>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="改造日期"
                    android:textSize="18sp"
                    android:textColor="#919191"/>
                <com.energyfuture.symphony.m3.ui.MyEditText
                    android:id="@+id/updatetime"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp"
                    android:background="@null"
                    android:singleLine="true"
                    android:textColorHint="#DDDDDC"
                    android:textColor="#000000"
                    android:focusableInTouchMode="false"
                    android:editable="false"
                    android:layout_marginTop="10dp"
                    android:tag="改造日期"/>
            </LinearLayout>
        </android.support.v7.widget.CardView>
    </RelativeLayout>

</android.support.v4.widget.DrawerLayout>
