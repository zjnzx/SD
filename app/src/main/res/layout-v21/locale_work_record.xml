<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/work_record_ll1"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <include
                    layout="@layout/tool_bar"/>
                <include
                    android:id="@+id/work_record_row_name"
                    layout="@layout/task_detail"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:background="#ffffff"
                android:padding="20dp">
                <Button
                    android:id="@+id/work_record_submit"
                    android:layout_width="400dp"
                    android:layout_height="55dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:text="登记"
                    android:textSize="18sp"
                    android:textColor="#ffffff"
                    android:background="@drawable/affirm_button_sel"/>
                <com.energyfuture.symphony.m3.ui.MyEditText
                    android:id="@+id/work_record_et1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="18dp"
                    android:paddingBottom="5dp"
                    android:editable="false"
                    android:background="@null"
                    android:textColor="#000000"
                    android:text="现场作业情况登记"
                    />
                <android.support.v4.widget.SwipeRefreshLayout
                    android:id="@+id/work_swipe"
                    android:layout_below="@id/work_record_et1"
                    android:layout_above="@id/work_record_submit"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="10dp">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="150dp">
                                <RelativeLayout
                                    android:id="@+id/work_record_rl1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">
                                    <View
                                        android:layout_width="30dp"
                                        android:layout_height="1dp"
                                        android:background="#F2F2F2"
                                        android:layout_centerVertical="true"/>
                                    <TextView
                                        android:id="@+id/text1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="天气"
                                        android:textSize="20sp"
                                        android:textColor="#ccc"
                                        android:layout_marginLeft="30dp"/>
                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:background="#F2F2F2"
                                        android:layout_toRightOf="@id/text1"
                                        android:layout_centerVertical="true"/>
                                </RelativeLayout>
                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#F2F2F2"
                                    android:layout_marginTop="14dp"/>
                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#F2F2F2"
                                    android:layout_marginTop="14dp"
                                    android:layout_alignParentRight="true"/>
                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="#F2F2F2"
                                    android:layout_alignParentBottom="true"/>
                                <RadioGroup
                                    android:id="@+id/work_record_rg1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/work_record_rl1"
                                    android:orientation="horizontal"
                                    android:layout_marginTop="10dp"
                                    android:paddingRight="30dp"
                                    android:paddingLeft="30dp">
                                    <RadioButton
                                        android:id="@+id/work_record_rb1"
                                        android:layout_weight="1"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:text="晴天"
                                        android:checked="true"
                                        android:textColor="#000000"
                                        android:textSize="20sp"
                                        android:gravity="center_vertical"/>
                                    <RadioButton
                                        android:id="@+id/work_record_rb2"
                                        android:layout_weight="1"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:text="阴天"
                                        android:textSize="20sp"
                                        android:textColor="#000000"
                                        android:gravity="center_vertical"/>
                                    <RadioButton
                                        android:id="@+id/work_record_rb3"
                                        android:layout_weight="1"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:text="雨天"
                                        android:textSize="20sp"
                                        android:textColor="#000000"
                                        android:gravity="center_vertical"/>
                                </RadioGroup>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/work_record_rg1"
                                    android:gravity="center_vertical"
                                    android:layout_marginTop="30dp"
                                    android:paddingLeft="30dp"
                                    android:paddingRight="30dp">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="温度："
                                        android:textSize="16sp"
                                        android:textColor="#000000"
                                        />
                                    <com.energyfuture.symphony.m3.ui.MyEditText
                                        android:id="@+id/temperature"
                                        android:layout_width="50dp"
                                        android:layout_height="wrap_content"
                                        android:paddingBottom="5dp"
                                        android:background="@null"
                                        android:singleLine="true"
                                        android:digits="1234567890-."
                                        android:inputType="numberDecimal"
                                        android:textColor="#000000"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="℃"
                                        android:textColor="#919191"
                                        android:textSize="16sp"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="湿度："
                                        android:textSize="16sp"
                                        android:textColor="#000000"
                                        android:layout_marginLeft="20dp"/>
                                    <com.energyfuture.symphony.m3.ui.MyEditText
                                        android:id="@+id/humidity"
                                        android:layout_width="50dp"
                                        android:layout_height="wrap_content"
                                        android:paddingBottom="5dp"
                                        android:background="@null"
                                        android:singleLine="true"
                                        android:digits="1234567890-."
                                        android:inputType="numberDecimal"
                                        android:textColor="#000000"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="%"
                                        android:textColor="#919191"
                                        android:textSize="16sp"/>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="时间："
                                        android:textSize="16sp"
                                        android:textColor="#000000"
                                        android:layout_marginLeft="20dp"/>
                                    <com.energyfuture.symphony.m3.ui.MyEditText
                                        android:id="@+id/time"
                                        android:layout_width="180dp"
                                        android:layout_height="wrap_content"
                                        android:paddingBottom="5dp"
                                        android:background="@null"
                                        android:editable="false"
                                        android:focusableInTouchMode="false"
                                        android:textColor="#000000"/>
                                </LinearLayout>
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_marginBottom="10dp">
                                <RelativeLayout
                                    android:id="@+id/work_record_rl2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">
                                    <View
                                        android:layout_width="30dp"
                                        android:layout_height="1dp"
                                        android:background="#F2F2F2"
                                        android:layout_centerVertical="true"/>
                                    <TextView
                                        android:id="@+id/text2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="变压器信息"
                                        android:textSize="20sp"
                                        android:textColor="#ccc"
                                        android:layout_marginLeft="30dp"/>
                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:background="#F2F2F2"
                                        android:layout_toRightOf="@id/text2"
                                        android:layout_centerVertical="true"/>
                                </RelativeLayout>
                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#F2F2F2"
                                    android:layout_marginTop="14dp"/>
                                <View
                                    android:layout_width="1dp"
                                    android:layout_height="match_parent"
                                    android:background="#F2F2F2"
                                    android:layout_marginTop="14dp"
                                    android:layout_alignParentRight="true"/>
                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:background="#F2F2F2"
                                    android:layout_alignParentBottom="true"/>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/work_record_rl2"
                                    android:layout_marginTop="10dp"
                                    android:orientation="vertical"
                                    android:layout_marginLeft="30dp"
                                    android:layout_marginRight="30dp">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="变压器名称"
                                        android:textColor="#919191"
                                        android:textSize="16sp"
                                        android:layout_marginTop="10dp"/>
                                    <com.energyfuture.symphony.m3.ui.MyEditText
                                        android:id="@+id/transformer_name"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:maxLength="32"
                                        android:paddingBottom="5dp"
                                        android:background="@null"
                                        android:singleLine="true"
                                        android:textColor="#000000"
                                        android:layout_marginTop="10dp"/>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp">
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical"
                                            android:paddingRight="5dp">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="出厂编号"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_number"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:maxLength="64"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:textColor="#000000"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="连接组标号"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_label"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:maxLength="20"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:textColor="#000000"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                    </LinearLayout>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="型号"
                                        android:textColor="#919191"
                                        android:textSize="16sp"
                                        android:layout_marginTop="10dp"/>
                                    <com.energyfuture.symphony.m3.ui.MyEditText
                                        android:id="@+id/transformer_model"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:maxLength="50"
                                        android:paddingBottom="5dp"
                                        android:background="@null"
                                        android:digits="1234567890abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
                                        android:singleLine="true"
                                        android:textColor="#000000"
                                        android:layout_marginTop="10dp"/>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp">
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical"
                                            android:paddingRight="5dp">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="相数"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_phase"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:maxLength="10"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:textColor="#000000"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="频率"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <RelativeLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="10dp">
                                                <com.energyfuture.symphony.m3.ui.MyEditText
                                                    android:id="@+id/transformer_HZ"
                                                    android:layout_width="200dp"
                                                    android:layout_height="wrap_content"
                                                    android:maxLength="20"
                                                    android:paddingBottom="5dp"
                                                    android:background="@null"
                                                    android:singleLine="true"
                                                    android:inputType="numberDecimal"
                                                    android:textColor="#000000"/>
                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:text="HZ"
                                                    android:layout_toRightOf="@id/transformer_HZ"
                                                    android:textColor="#919191"
                                                    android:textSize="16sp"
                                                    android:layout_marginLeft="5dp"/>
                                            </RelativeLayout>
                                        </LinearLayout>
                                    </LinearLayout>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="额定电压"
                                        android:textColor="#919191"
                                        android:textSize="16sp"
                                        android:layout_marginTop="10dp"/>
                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp">
                                        <com.energyfuture.symphony.m3.ui.MyEditText
                                            android:id="@+id/transformer_voltage"
                                            android:layout_width="435dp"
                                            android:layout_height="wrap_content"
                                            android:maxLength="50"
                                            android:paddingBottom="5dp"
                                            android:background="@null"
                                            android:singleLine="true"
                                            android:digits="1234567890.+-*/%_()"
                                            android:inputType="numberSigned"
                                            android:textColor="#000000"/>
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="kV"
                                            android:layout_toRightOf="@id/transformer_voltage"
                                            android:textColor="#919191"
                                            android:textSize="16sp"
                                            android:layout_marginLeft="5dp"/>
                                    </RelativeLayout>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="额定容量"
                                        android:textColor="#919191"
                                        android:textSize="16sp"
                                        android:layout_marginTop="10dp"/>
                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp">
                                        <com.energyfuture.symphony.m3.ui.MyEditText
                                            android:id="@+id/transformer_capacity"
                                            android:layout_width="420dp"
                                            android:layout_height="wrap_content"
                                            android:maxLength="50"
                                            android:paddingBottom="5dp"
                                            android:background="@null"
                                            android:singleLine="true"
                                            android:digits="1234567890.+-*/%_()"
                                            android:inputType="numberSigned"
                                            android:textColor="#000000"/>
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="MVA"
                                            android:layout_toRightOf="@id/transformer_capacity"
                                            android:textColor="#919191"
                                            android:textSize="16sp"
                                            android:layout_marginLeft="5dp"/>
                                    </RelativeLayout>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp">
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical"
                                            android:paddingRight="5dp">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="使用条件"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_conditions"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:maxLength="20"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:textColor="#000000"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="变压器油"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_oil"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:maxLength="20"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:inputType="numberSigned"
                                                android:textColor="#000000"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp">
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical"
                                            android:paddingRight="5dp">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="冷却方式"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_cooling"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:maxLength="20"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:textColor="#000000"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="调压方式"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_pressure"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:maxLength="50"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:textColor="#000000"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                    </LinearLayout>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="制造厂"
                                        android:textColor="#919191"
                                        android:textSize="16sp"
                                        android:layout_marginTop="10dp"/>
                                    <com.energyfuture.symphony.m3.ui.MyEditText
                                        android:id="@+id/transformer_Factory"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:maxLength="100"
                                        android:paddingBottom="5dp"
                                        android:background="@null"
                                        android:singleLine="true"
                                        android:textColor="#000000"
                                        android:layout_marginTop="10dp"/>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp">
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical"
                                            android:paddingRight="5dp">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="制造年月"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_make"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:textColor="#000000"
                                                android:editable="false"
                                                android:focusableInTouchMode="false"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">
                                            <TextView
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:text="投运日期"
                                                android:textSize="16sp"
                                                android:textColor="#919191"/>
                                            <com.energyfuture.symphony.m3.ui.MyEditText
                                                android:id="@+id/transformer_commissioning"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:paddingBottom="5dp"
                                                android:background="@null"
                                                android:singleLine="true"
                                                android:textColor="#000000"
                                                android:editable="false"
                                                android:focusableInTouchMode="false"
                                                android:layout_marginTop="10dp"/>
                                        </LinearLayout>
                                    </LinearLayout>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="改造日期"
                                        android:textColor="#919191"
                                        android:textSize="16sp"
                                        android:layout_marginTop="10dp"/>
                                    <com.energyfuture.symphony.m3.ui.MyEditText
                                        android:id="@+id/transformer_remould"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:paddingBottom="5dp"
                                        android:background="@null"
                                        android:textColor="#000000"
                                        android:editable="false"
                                        android:focusableInTouchMode="false"
                                        android:layout_marginTop="10dp"/>
                                </LinearLayout>
                            </RelativeLayout>

                        </LinearLayout>
                    </ScrollView>
                </android.support.v4.widget.SwipeRefreshLayout>
            </RelativeLayout>
            </LinearLayout>
        <LinearLayout
            android:id="@+id/background"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#30000000"
            android:visibility="gone"/>
        </FrameLayout>
</LinearLayout>