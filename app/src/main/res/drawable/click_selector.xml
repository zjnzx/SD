<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:state_pressed="false">
        <shape>
            <!-- 默认背景颜色 -->
            <solid android:color="#EAEFF2" />
        </shape>
    </item>
    <item
        android:state_pressed="true"  >
        <shape>
            <!-- 按下背景颜色 -->
            <solid android:color="#F1F5F8" />
        </shape>
    </item>
</selector>
